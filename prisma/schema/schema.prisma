generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum SectionType {
  LECTURE
  PRACTICE
}

enum CourseGroup {
  GENED
  FREE_ELEC
  SPEC
}

enum CourseSubGroup {
  FREE_ELEC
  FUND
  LANG
  FAC_SPEC
  GENED_ELEC
  CORE
  REQUIRED
  ELEC_REQ
  ALT_STUDY
  MAJOR_ELEC
}

enum DayOfWeek {
  MONDAY
  TUESDAY
  WEDNESDAY
  THURSDAY
  FRIDAY
  SATURDAY
  SUNDAY
}

enum UserRole {
  ADMIN
  USER
}

enum OAuthProvider {
  GOOGLE
}


